generator client {
  provider = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
  output   = "./../src/generated/prisma"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model User {
  id       Int      @id @default(autoincrement())
  name     String
  email    String   @unique
  password String
}
